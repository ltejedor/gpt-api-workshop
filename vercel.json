{
    "version": 2,
    "builds": [
      { "src": "public/**", "use": "@vercel/static" },
      { "src": "api/completions.js", "use": "@vercel/node" },
      { "src": "api/image_gen.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/completions", "dest": "/api/completions.js" },
      { "src": "/api/image_gen", "dest": "/api/image_gen.js" },
      { "src": "/(.*)", "dest": "/public/$1" }
    ]
  }